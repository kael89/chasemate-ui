{
  "name": "chasemate-ui",
  "version": "0.1.0",
  "description": "Front-end of Chasemate, a web app that calculates forced mates in a given chess position",
  "homepage": "https://chasemate.app",
  "author": "Kostas Karvounis",
  "license": "GPL 3.0",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint \"**/*.{js,jsx}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix & prettier \"**/*.{json,md}\" --write --ignore-path .gitignore",
    "test": "react-scripts test",
    "test:e2e": "npm run start & wait-on http://localhost:3000 && cypress run",
    "test:all": "CI=true npm test; npm run test:e2e",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "cross-fetch": "^3.0.1",
    "lodash": "^4.17.11",
    "prism-react-renderer": "^0.1.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dom": "^16.8.2",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "wait-on": "^3.3.0"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "cypress": "^3.3.1",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-simple-import-sort": "^3.0.0",
    "prettier": "^1.16.4"
  }
}
